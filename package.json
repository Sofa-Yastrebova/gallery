{
  "name": "gallery",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview --open",
    "prepare": "husky",
    "prettier": "prettier 'src/**/*.{js,jsx,ts,tsx,css,scss,md}' --write",
    "lint": "eslint --fix",
    "format": "npm run lint && npm run prettier",
    "deploy": "gh-pages -d dist"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx,css,scss,md}": [
      "npm run format"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
